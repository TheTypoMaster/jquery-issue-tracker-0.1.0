<html>
<head>
	<title>Repository Details</title>
	<link rel='stylesheet' href='/css/style.css' />
	<script src="https://code.jquery.com/jquery-1.10.2.js"></script>
	   <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
	<!-- // <script src="http://d3js.org/d3.v3.min.js" charset="utf-8"></script> -->

    <script src="/js/scrilla.js"></script>

</head>
<body>
	<%-      visualHelper.getBarChart({
        data: ch,
        width: 400,
        height: 300,
        xAxisLabel: '2012',
        yAxisLabel: 'Views',
        containerId: 'bar-chart-small'
      });
    %>
    <hr>
    <%-
      visualHelper.getBarChart({
        data: ch,
        width: 800,
        height: 600,
        xAxisLabel: '2012',
        yAxisLabel: 'Views',
        containerId: 'bar-chart-large'
      });
    %>
<p>Repository name: <%= data.name %></p>
<p>Open Issues: <%= data.open_issues %>&nbsp;<a href=../issue/details/<%= data.name %>>Details</a></p>
<p>Watchers: <%= data.watchers_count %></p>
<p>Created at: <%= data.created_at %></p>
<p>Description: <%= data.description %></p>
<p>Number of forks: <%= data.forks_count %></p>
<p>Pull Requests: <%= pullRequestNo %></p>
<p>Closed Pull Requests: <%= closedPulls %></p><i class="cent"></i>
<p></p>
<p>Commits URL: <a href=<%= url.commits %>><%= url.commits %></a></p>
<div class="button-wrap-static">
<% for (var i = 0; i < history.length; i++) { %>
   <%      
   	date = history[i].split(' ');
   	cd = date[2]+ " " + date[1] + " " + date[3]
   %>
<div  class='open-issue-repo'><%= cd %><span class='issue-count'><%= issuesTrend[i] %></span></div>  
<% i} %> 
</div>

<p><!-- <svg height="100" width="100">
  <circle cx="50" cy="50" r="40" stroke="black" stroke-width="3" fill="red" />
</svg> --></p>
</body>
</html>
